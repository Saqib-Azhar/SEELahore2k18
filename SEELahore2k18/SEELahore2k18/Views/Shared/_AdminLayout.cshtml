    <script>
        $(document).ready(function () {
            var url = window.location.pathname.split("/");
            var currentId = url[1];
            var prevId = $(".active").attr("id");
            $("#" + prevId).removeClass("active");
            $("#tab_" + currentId).addClass("active");
            $("#tab_" + currentId).focus();

        });
